{
    "window.zoomLevel": 1,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Snazzy Light",
    "editor.tokenColorCustomizations": {
        "[Snazzy Light]": {
            "comments": {
                "fontStyle": "italic",
                "foreground": "#008000",
            }
        }
    },
    "html.format.wrapLineLength": 200,
    "phpfmt.passes": [
        "AlignDoubleArrow",
        "AlignPHPCode",
        "DoubleToSingleQuote",
        "ShortArray"
    ],
}
