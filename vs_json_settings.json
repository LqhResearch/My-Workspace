{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Dracula",
    "editor.tokenColorCustomizations": {
        "[Dracula]": {
            "comments": {
                "fontStyle": "italic",
                "foreground": "#999999"
            }
        }
    },
    "html.format.wrapLineLength": 256,
    "phpfmt.passes": [
        "AlignDoubleArrow",
        "AlignPHPCode",
        "DoubleToSingleQuote",
        "ShortArray"
    ],
    "files.autoSave": "afterDelay",
    "tabnine.experimentalAutoImports": true,
    "window.zoomLevel": 1
}